<view class="contianer">
  <view class="wrapper">
    <view class="welcome">
      欢迎回来!
    </view>
    <view class="input-content">
      <text class="phone">手机号</text>
      <input id='phone' bindinput="hanldinput"  type="text" placeholder="请输入手机号"/>
    </view>
    
    <view class="input-password">
    <text class="password">密码</text>
    <input id="password" bindinput="hanldinput" type="password" placeholder="请输入密码"/>
    </view>
   
    
  </view>
  <button class="confirm-btn" bindtap="login">登录</button>
  <view class="forget">
    忘记密码
  </view>
  <view class="register">
  还没有账号?
  <text>马上注册</text>
  </view>
</view>
